OBJS=	main.o block.o get.o put.o internal.o
USPET=	../ets/usp_et.o

ProfiledObjectRule()

all::	libusp.a libusp_p.a

install_library(usp,$(ATHLIBDIR))

libusp.a: $(OBJS) $(USPET)
	ar cru $@ $(OBJS) $(USPET)
	$(RANLIB) $@

libusp_p.a: $(OBJS) $(USPET)
	cd profiled; ar cru ../$@ $(OBJS) ../$(USPET)
	$(RANLIB) $@
